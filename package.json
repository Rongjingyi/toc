{
  "name": "falsework-with-next7",
  "scripts": {
    "build": "next build",
    "dev:m": "cross-env NODE_ENV=development node --experimental-modules server.mjs",
    "dev": "cross-env NODE_ENV=development node --experimental-modules server.mjs",
    "pro:m": "npm run build && cross-env NODE_ENV=production node --experimental-modules --no-warnings server.mjs"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "antd": "^3.11.6",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.0",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-status-monitor": "^1.2.3",
    "express-winston": "^3.0.1",
    "file-loader": "^3.0.1",
    "next": "^7.0.2",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "sass-loader": "^7.1.0",
    "styled-jsx": "^3.1.3",
    "styled-jsx-plugin-sass": "^1.0.0",
    "url-loader": "^1.1.2",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.5.1"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "import",
