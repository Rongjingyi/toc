{
  "name": "@junjie.lean/toc",
  "version": "0.1.3",
  "description": "A Middleware-SSR Framework Based of NEXT and Express",
  "main": "server.js",
  "bin":"server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node  server.js",
    "eject": "cross-env NODE_ENV=development node util/eject.js",
    "pro": "npm run build && cross-env NODE_ENV=production node --no-warnings server.js",
    "build": "next build",
    "dist": "pkg . -t win --out-dir dist ",
    "test": "jest"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^3.12.1",
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.0",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "express-status-monitor": "^1.2.3",
    "express-winston": "^3.0.1",
    "file-loader": "^3.0.1",
    "next": "^7.0.2",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "readline-sync": "^1.4.9",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "sass-loader": "^7.1.0",
    "signale": "^1.3.0",
    "styled-jsx": "^3.1.3",
    "styled-jsx-plugin-sass": "^1.0.0",
    "url-loader": "^1.1.2",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "babel-preset-minify": "^0.5.0",
    "jest": "^23.6.0",
    "pkg": "^4.3.7"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ]
    ],
    "comments": false
  },
  "author": {
    "name": "lean",
    "email": "junjie.lean@foxmail.com"
  },
  "pkg": {
    "assets": [
      ".toc/**/*"
    ],
    "scripts": [
      ".toc/server/**/*.js"
    ]
  },
  "repository": "https://github.com/junjie-lean/falseworkWithNext7",
  "license": "MIT"
}
